{
  "name": "hackoregon-frontend-starter",
  "version": "0.1.0",
  "description": "Hack Oregon Front-end React + Redux Starter",
  "author": "David Daniel",
  "license": "MIT",
  "keywords": [
    "react",
    "react-router",
    "webpack",
    "redux",
    "boilerplate",
    "starter",
    "hackoregon"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "clean": "rimraf ./build && mkdir build",
    "start": "npm-run-all --parallel test:watch start:dev lint:watch",
    "lint": "esw src tools server webpack.config.* --color",
    "lint:watch": "npm run lint -- --watch",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "analyze": "babel-node ./tools/analyzeBundle.js",
    "start:dev": "cross-env NODE_ENV=development babel-node server",
    "start:production": "npm run build && npm run start:prod:server",
    "start:prod:server": "cross-env NODE_ENV=production babel-node server",
    "test": "mocha --opts ./mocha.opts 'src/**/*.test.js'",
    "test:watch": "npm run test -- --watch",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --root src --report html npm run test --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js src/**/*.test.js && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "show:coverage": "npm run test:cover && open coverage/index.html"
  },
  "dependencies": {
    "@hackoregon/component-library": "0.0.6",
    "babel-polyfill": "6.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "express": "4.14.0",
    "invariant": "2.2.2",
    "isomorphic-fetch": "2.2.1",
    "ramda": "0.23.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "recharts": "0.22.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "styled-components": "1.4.2"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-enzyme": "0.6.1",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "coveralls": "2.11.15",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-babel": "4.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jsdom": "9.8.3",
    "jsdom-global": "2.1.0",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "mocha-clean": "^1.0.0",
    "mochawesome": "^1.5.1",
    "nock": "9.0.2",
    "npm-run-all": "3.1.2",
    "postcss-loader": "1.2.1",
    "react-addons-test-utils": "15.4.1",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.2.1",
    "rimraf": "2.5.4",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  }
}
