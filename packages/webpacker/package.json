{
  "name": "@hackoregon/webpacker",
  "version": "1.0.0-alpha.fe716b58",
  "description": "Compose your webpack configs - comes with a default",
  "author": "David Daniel <davidedaniel@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackoregon/webpacker"
  },
  "scripts": {
    "build:esm": "BABEL_ENV=esm ../../node_modules/.bin/babel src --out-dir es",
    "build:cjs": "BABEL_ENV=cjs ../../node_modules/.bin/babel src --out-dir dist",
    "build": "yarn build:cjs && yarn build:esm",
    "lint": "eslint --fix src"
  },
  "keywords": [
    "webpack",
    "ramda",
    "modular"
  ],
  "dependencies": {
    "@hackoregon/civic-babel-presets": "^1.0.0-alpha.fe716b58",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "postcss-loader": "^1.3.3",
    "ramda": "^0.23.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.1"
  }
}
