{
  "name": "@hackoregon/civic-emergency-response",
  "version": "1.0.0-alpha.1c62c05f",
  "description": "Hack Oregon Front-end React + Redux Starter",
  "author": "David Daniel",
  "license": "MIT",
  "keywords": [
    "react",
    "react-router",
    "webpack",
    "redux",
    "boilerplate",
    "starter",
    "hackoregon"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "clean": "rimraf ./build && mkdir build",
    "start": "npm-run-all --parallel test:watch start:dev",
    "lint": "eslint --fix src tools server webpack.config.* --color",
    "lint:watch": "npm run lint -- --watch",
    "build": "babel-node tools/build.js",
    "analyze": "babel-node ./tools/analyzeBundle.js",
    "start:dev": "npx -p babel-cli cross-env NODE_ENV=development babel-node server",
    "start:production": "npm run build && npm run start:prod:server",
    "start:prod:server": "cross-env NODE_ENV=production babel-node server",
    "test": "mocha --opts ./mocha.opts 'src/**/*.test.js'",
    "test:watch": "npm run test -- --watch",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --root src --report html npm run test --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js src/**/*.test.js && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "show:coverage": "npm run test:cover && open coverage/index.html"
  },
  "dependencies": {
    "@hackoregon/component-library": "^1.0.0-alpha.1c62c05f",
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "express": "^4.14.1",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^1.0.3",
    "ramda": "^0.23.0",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-fontawesome": "^1.6.1",
    "react-leaflet": "^1.1.6",
    "react-player": "^0.15.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-form": "^6.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0",
    "styled-components": "^1.4.2"
  },
