    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.5.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-transform-hmr": "^1.0.4",
