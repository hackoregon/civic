{
  "name": "@hackoregon/civic-platform",
  "version": "1.0.0-alpha.fe716b58",
  "description": "Civic - A Hack Oregon Project",
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "contributors": [
    "David Daniel <davidedaniel@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./build",
    "dev": "../../node_modules/.bin/babel-node tools/dev",
    "build": "NODE_ENV=production webpack --config webpack.config.babel.js --colors",
    "build:server": "babel src --out-dir build --copy-files",
    "build:prod": "NODE_ENV=production run-s build build:server",
    "build:all": "BABEL_ENV=production run-s clean build build:server",
    "lint": "eslint --fix src",
    "start": "NODE_ENV=production npm run go",
    "serve:app": "../../node_modules/.bin/babel-node build/server/index.js",
    "serve:static": "BABEL_ENV=production ../../node_modules/.bin/babel-node staticServer/index.js",
    "preserve:server": "npm run build:server",
    "go": "BABEL_ENV=production run-p serve:static serve:app",
    "test": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hackoregon/civic-platform"
  },
  "dependencies": {
    "@hackoregon/civic-emergency-response": "^1.0.0-alpha.fe716b58",
    "@hackoregon/civic-housing": "^1.0.0-alpha.f1b97411",
    "@hackoregon/civic-server": "0.0.10",
    "@hackoregon/component-library": "^1.0.0-alpha.fe716b58",
    "@hackoregon/webpacker": "^1.0.0-alpha.fe716b58",
    "app-root-dir": "^1.0.2",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.24.1",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "css-modules-require-hook": "^4.0.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "isomorphic-loader": "^1.6.2",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.0.2",
    "pm2": "^2.4.6",
    "postcss-loader": "^1.3.3",
    "ramda": "^0.23.0",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "reselect": "^3.0.0",
    "rxjs": "^5.1.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.1",
    "webpack-md5-hash": "^0.0.5"
  }
}
