{
  "name": "@hackoregon/civic-housing",
  "version": "1.0.0-alpha.fe716b58",
  "description": "Civic/Housing 2017",
  "main": "src/index.js",
  "contributors": [
    "Noah Muldavin",
    "Michael Lange",
    "John Tasto",
    "David Daniel",
    "Megan McKissack",
    "Derek DeMaria",
    "Pat",
    "Schola Choi"
  ],
  "license": "MIT",
  "keywords": [
    "react",
    "react-router",
    "redux",
    "hackoregon"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "clean": "rimraf build && mkdir build",
    "start": "yarn start:dev",
    "lint": "eslint --fix src tools server webpack.config.* --color",
    "lint:watch": "npm run lint -- --watch",
    "build": "rimraf dist && cross-env NODE_ENV=production ../../node_modules/.bin/babel-node ./tools/build.js",
    "analyze": "../../node_modules/.bin/babel-node ./tools/analyzeBundle.js",
    "prestart:dev": "npm run clean",
    "start:dev": "cross-env BABEL_ENV=development ../../node_modules/.bin/babel-node server",
    "start:prod": "cross-env BABEL_ENV=production node server",
    "test": "cross-env BABEL_ENV=test mocha --opts ./mocha.opts 'src/**/*.test.js'",
    "test:watch": "npm run test -- --watch",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --root src --report html npm run test --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js src/**/*.test.js && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "show:coverage": "npm run test:cover && open coverage/index.html",
    "prebump": "NODE_ENV=production npm run build",
    "bump": "bump --prompt --commit",
    "postbump": "editme -w"
  },
  "dependencies": {
    "@hackoregon/civic-server": "0.0.10",
    "@hackoregon/component-library": "^1.0.0-alpha.fe716b58",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "d3-format": "^1.2.0",
    "express": "^4.14.1",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^1.0.3",
    "query-string": "^4.3.3",
    "ramda": "^0.23.0",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-helmet": "^5.0.3",
    "react-leaflet": "^1.1.6",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0",
    "styled-components": "^1.4.2"
  },
  "devDependencies": {
    "@hackoregon/civic-babel-presets": "^1.0.0-alpha.fe716b58",
    "@hackoregon/webpacker": "^1.0.0-alpha.fe716b58"
  }
}
