{
  "name": "@hackoregon/civic-budget",
  "version": "1.0.0-alpha.fe716b58",
  "description": "Civic/Budget 2017",
  "author": "David Daniel",
  "license": "MIT",
  "keywords": [
    "react",
    "react-router",
    "webpack",
    "redux",
    "boilerplate",
    "starter",
    "hackoregon"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "clean": "rimraf ./build && mkdir build",
    "start": "yarn start:dev",
    "lint": "eslint --fix src tools server webpack.config.* --color",
    "lint:watch": "npm run lint -- --watch",
    "build": "cross-env NODE_ENV=production ../../node_modules/.bin/babel-node tools/build.js",
    "analyze": "../../node_modules/.bin/babel-node ./tools/analyzeBundle.js",
    "start:dev": "cross-env NODE_ENV=development ../../node_modules/.bin/babel-node server",
    "start:production": "npm run build && npm run start:prod:server",
    "start:prod:server": "cross-env NODE_ENV=production ../../node_modules/.bin/babel-node server",
    "test": "mocha --opts ./mocha.opts './src/**/*.test.js'",
    "test:watch": "yarn test --watch",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --root src --report html npm run test --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js src/**/*.test.js && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "show:coverage": "npm run test:cover && open coverage/index.html"
  },
  "dependencies": {
    "@hackoregon/component-library": "^1.0.0-alpha.fe716b58",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "d3": "^4.8.0",
    "express": "^4.14.1",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "ramda": "^0.23.0",
    "rc-slider": "^7.0.2",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.0.0-rc.3",
    "react-sticky": "^5.0.8",
    "recharts": "^0.22.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0",
    "styled-components": "^1.4.2"
  },
  "devDependencies": {
    "@hackoregon/civic-babel-presets": "^1.0.0-alpha.fe716b58",
    "@hackoregon/webpacker": "^1.0.0-alpha.fe716b58"
  }
}
